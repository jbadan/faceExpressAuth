<div class="container-fluid">

<h1> Hello, <%=user.name%></h1>
  <div class=row>
    <div class="col-md-12">
      <% user.cloudinaries.forEach(function(cloudinary){ %>
      <% var imgUrl = cloudinary2.url(cloudinary.src, {width: 250, height: 250}); %>
      <img id="image" class="img-responsive" src="<%= imgUrl %>">
      <% }) %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12 addMore">
        <h2> <i class="glyphicon glyphicon-picture"></i> Want more? Choose another image:</h2>
        <form enctype="multipart/form-data" action="/profile/" method="post">
          <input type="file" name="myFile">
          <input type="submit" class="btn btn-default">
        </form>
    </div>
  </div>
</div>
