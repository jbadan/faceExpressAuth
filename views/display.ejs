<div class="container-fluid" id="display">
	<div class="jumbotron" id="jumbotron">
		<h1 id="header">Check you out!</h1>
			<% images.forEach(function(image){ %>
			<% var imgUrl = cloudinary.url(image, {width: 450, height: 450}); %>
			<img class="img-responsive col-lg-offset-3 inputImage" id="inputImage" src="<%= imgUrl %>">
			<% }) %>
			<br>
			<button class="btn btn-default btn-lg" id="renderCharts" onclick="processImage()">Show me the results!</button>
	</div>
		<div class="row imageCharts">
			<div class="col-md-3">
				<% images.forEach(function(image){ %>
				<% var imgUrl = cloudinary.url(image, {width: 250, height: 250}); %>
				<img id="image" class="img-responsive" src="<%= imgUrl %>">
				<% }) %>
			</div>
			<div class="col-md-9">
				<div id="myCarousel" class="carousel slide" data-ride="carousel">
		  		<ol class="carousel-indicators">
				    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
				    <li data-target="#myCarousel" data-slide-to="1"></li>
				    <li data-target="#myCarousel" data-slide-to="2"></li>
		  		</ol>
				  <div class="carousel-inner">
				    <div class="item active">
							<div class="chart-container">
									 <canvas id="emotionChart"></canvas>
							</div>
				    </div>
				    <div class="item">
							<div class="chart-container">
									 <canvas id="hairChart"></canvas>
							</div>
				    </div>
				    <!-- <div class="item">
				      <img src="ny.jpg" alt="New York">
				    </div> -->
				  </div>
				  <a class="left carousel-control" href="#myCarousel" data-slide="prev">
				    <span class="glyphicon glyphicon-chevron-left" style="color:#F2C777"></span>
				    <span class="sr-only">Previous</span>
				  </a>
				  <a class="right carousel-control" href="#myCarousel" data-slide="next">
				    <span class="glyphicon glyphicon-chevron-right" style="color:#F2C777"></span>
				    <span class="sr-only">Next</span>
				  </a>
				</div>
			</div>
	</div>
	<div class="row imageCharts">
		<div class="col-md-12 addMore">
				<a href="/">Back Home</a>
		</div>
	</div>
</div>
